  "steps": [
    {
      "id": "STEP-023",
      "stepNumber": 1,
      "instruction": "Preheat oven to 350°F. If time allows, heavily season cubed chicken breast with kosher salt and let sit at room temperature for 20+ minutes to dry brine.",
      "estimatedTime": 1200
    },
    {
      "id": "STEP-024",
      "stepNumber": 2,
      "instruction": "Place artichoke hearts in a dish towel and squeeze out excess liquid. Finely chop and set aside. Repeat with thawed spinach to remove all excess moisture.",
      "estimatedTime": 300
    },
    {
      "id": "STEP-025",
      "stepNumber": 3,
      "instruction": "Heat avocado oil in a large pan over medium heat. Add chicken pieces in single layer, season with black pepper, and cook 2-3 minutes per side until mostly done. Remove and set aside.",
      "estimatedTime": 480
    },
    {
      "id": "STEP-026",
      "stepNumber": 4,
      "instruction": "In a small food processor, blend cottage cheese, garlic, salt, pepper, and onion powder until smooth.",
      "estimatedTime": 180
    },
    {
      "id": "STEP-027",
      "stepNumber": 5,
      "instruction": "In baking dish, combine cooked chicken, spinach, artichokes, blended cottage cheese mixture, and ⅔ of the mozzarella and parmesan. Mix and spread evenly.",
      "estimatedTime": 300
    },
    {
      "id": "STEP-028",
      "stepNumber": 6,
      "instruction": "Cover with foil and bake for 20 minutes. Remove foil, sprinkle remaining cheese on top, and bake uncovered for 10 more minutes.",
      "estimatedTime": 1800
    },
    {
      "id": "STEP-029",
      "stepNumber": 7,
      "instruction": "Optional: Switch to broil for 2-3 minutes at the end to achieve golden, bubbly crust. Let rest briefly before serving.",
      "estimatedTime": 300
    }
  ]
}
